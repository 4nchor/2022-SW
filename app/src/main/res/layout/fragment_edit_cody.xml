<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="startApp_1.cody.editCodyFrag">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <EditText
            android:id="@+id/editCodyname_et"
            android:layout_width="match_parent"
            android:hint="코디 세트 이름 입력"
            android:layout_height="50dp"></EditText>

        <Button
            android:id="@+id/editHashTag_btn3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="태그 수정하기" />

        <Button
            android:id="@+id/editCodyClothes_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="의류 변경"
            tools:layout_editor_absoluteY="" />

        <Button
            android:id="@+id/doneeditCody_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="수정 완료"
            tools:layout_editor_absoluteY="" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
